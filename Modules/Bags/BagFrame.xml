<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Script file="BagFrame.lua" />

    <Frame name="BagFrameTemplate" mixin="BagFrameMixin" enableMouse="true" frameStrata="MEDIUM" topLevel="true" hidden="true" movable="true" clampedToScreen="true" inherits="ButtonFrameTemplate" virtual="true">
        <Size x="384" y="512" />

        <Frames>
            <EditBox name="$parentSearchBox" parentKey="SearchBox" inherits="SearchBoxTemplate">
                <Size x="158" y="18" />
                <Anchors>
                    <Anchor point="LEFT" relativePoint="TOPLEFT" x="70" y="-42" />
                    <Anchor point="RIGHT" relativePoint="TOPRIGHT" x="-15" y="-42" />
                </Anchors>
                <Scripts>
                    <OnTextChanged function="BagFrame_SearchBox_OnTextChanged" />
                </Scripts>
            </EditBox>
        </Frames>

        <Layers>
            <Layer level="OVERLAY">
                <FontString name="$parentTitle" parentKey="Title" inherits="GameFontHighlight" justifyH="MIDDLE">
                    <Anchors>
                        <Anchor point="TOP" x="0" y="-6" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>

        <Scripts>
            <OnLoad function="BagFrame_OnLoad" />
        </Scripts>
    </Frame>
</Ui>
