<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Script file="Inventory.lua" />

    <Frame name="InventoryFrameTemplate" mixin="InventoryMixin" parent="UIParent" inherits="BagFrameTemplate" virtual="true">
        <Size x="384" y="512" />
        <Anchors>
            <Anchor point="BOTTOMRIGHT" x="-100" y="100"/>
        </Anchors>

        <Frames>
            <Frame name="$parentCurrency" mixin="InventoryMoneyMixin" parentKey="Money" inherits="MoneyFrameTemplate">
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" x="0" y="5"/>
                </Anchors>

                <Scripts>
                    <OnEnter method="Money_OnEnter" />
                </Scripts>
            </Frame>
        </Frames>

        <Scripts>
            <OnLoad method="OnLoad" />
            <OnHide method="OnHide" />
        </Scripts>
    </Frame>
</Ui>
